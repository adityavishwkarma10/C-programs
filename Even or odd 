// Program to check whether number is even or odd
#include <stdio.h>

int main() {
    int num;
    printf("Enter the number that you want to check even or odd:-");
    scanf("%d",&num);
    if (num%2==0){
        printf("Number %d is even",num);
    }
    else
    printf("Number %d is odd",num);

    return 0;
}